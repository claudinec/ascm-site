{
  "name": "drupal-composer/drupal-project",
  "description": "Composer template for Drupal projects",
  "repositories": [{
    "type": "composer",
    "url": "https://packages.drupal.org/7"
  }],
  "require": {
    "composer/installers": "~1.0",
    "derhasi/composer-preserve-paths": "0.1.*",
    "drupal/drupal": "7.*",
    "drupal/addressfield": "~7.1.0",
    "drupal/addressfield_phone": "^1.3",
    "drupal/addressfield_tokens": "^1.5",
    "drupal/admin_menu": "^7.3@RC",
    "drupal/advanced_help": "~7.1.3",
    "drupal/autocomplete_widgets": "^7.1@RC",
    "drupal/auto_nodetitle": "^7.1",
    "drupal/backup_migrate": "^3.1",
    "drupal/bootstrap": "^7.3",
    "drupal/boxes": "^7.1",
    "drupal/chart": "^7.1",
    "drupal/ctools": "^7.1",
    "drupal/context": "^7.3",
    "drupal/date": "^7.2",
    "drupal/devel": "^7.1",
    "drupal/diff": "^7.3",
    "drupal/disqus": "^1.12",
    "drupal/ds": "^7.2",
    "drupal/email": "^7.1",
    "drupal/entity": "^1.8",
    "drupal/entityreference": "^1.2",
    "drupal/environment_indicator": "^2.8",
    "drupal/fb": "^7.3",
    "drupal/features": "^7.2",
    "drupal/filefield_paths": "^7.1",
    "drupal/filefield_sources": "^7.1",
    "drupal/google_analytics": "^7.2",
    "drupal/google_analytics_reports": "^7.3",
    "drupal/honeypot": "^1.22",
    "drupal/imce": "^7.1",
    "drupal/imce_filefield": "^7.1",
    "drupal/imce_wysiwyg": "^7.1",
    "drupal/link": "^1.4",
    "drupal/linkchecker": "^1.3",
    "drupal/logintoboggan": "^7.1",
    "drupal/mollom": "^7.2",
    "drupal/revisioning": "^7.1",
    "drupal/rules": "^2.9",
    "drupal/search404": "^1.4",
    "drupal/smtp": "^1.4",
    "drupal/token": "^1.6",
    "drupal/views": "^7.3",
    "mailchimp/mailchimp": "^2.0",
    "drupal/webform": "^7.4",
    "drupal/workbench": "7.1",
    "drupal/feeds": "2.x-dev",
    "drupal/colorbox": "^2.12",
    "drupal/querypath": "^2.1",
    "drupal/strongarm": "^2.0",
    "drupal/conditional_fields": "3.x-dev",
    "drupal/markdown": "^1.4",
    "drupal/media": "^1.5",
    "drupal/media_colorbox": "1.x-dev",
    "drupal/mimemail": "1.x-dev",
    "drupal/module_filter": "^2.0",
    "drupal/multiupload_filefield_widget": "^1.13",
    "drupal/oauth_common": "^3.2",
    "drupal/phone": "^1.0",
    "drupal/scheduler": "^1.5",
    "drupal/views_bulk_operations": "^3.3",
    "drupal/views_rss": "2.x-dev",
    "drupal/wysiwyg_fields": "2.x-dev",
    "drupal/pathauto": "^1.3",
    "drupal/typogrify": "1.x-dev",
    "drupal/xmlsitemap": "^2.3",
    "drupal/taxonomy_access": "^1.0",
    "drupal/twitter_block": "^2.3",
    "drupal/mailsystem": "^2.0",
    "drupal/mandrill_reports": "^2.3",
    "drupal/webform_mailchimp": "2.x-dev",
    "drupal/webform_phone": "^1.23",
    "drupal/libraries": "^2.3",
    "drupal/file_entity": "2.x-dev"
  },
  "minimum-stability": "dev",
  "conflict": {
    "drupal/core": "8.*"
  },
  "scripts": {
    "post-create-project-cmd": [
      "rm README.md LICENSE .travis.yml phpunit.xml.dist"
    ]
  },
  "config": {
    "vendor-dir": "vendor"
  },
  "extra": {
    "installer-paths": {
      "web/": ["type:drupal-core"],
      "web/sites/all/modules/contrib/{$name}/": ["type:drupal-module"],
      "web/sites/all/themes/contrib/{$name}/": ["type:drupal-theme"],
      "web/sites/all/libraries/{$name}/": ["type:drupal-library"],
      "web/sites/all/drush/{$name}/": ["type:drupal-drush"],
      "web/profiles/{$name}/": ["type:drupal-profile"]
    },
    "preserve-paths": [
      "web/sites/all/modules/contrib",
      "web/sites/all/themes/contrib",
      "web/sites/all/libraries",
      "web/sites/all/drush",
      "web/sites/default/settings.php",
      "web/sites/default/files"
    ]
  }
}
