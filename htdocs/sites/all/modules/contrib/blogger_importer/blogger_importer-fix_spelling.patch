From: Claudine Chionh <cc@claudinec.net>
Date: Fri, 13 Sep 2013 11:36:04 +1000
Subject: [PATCH] fix typos in blogger_importer

---
 batch.comments.inc | 2 +-
 batch.nodes.inc    | 2 +-
 2 files changed, 2 insertions(+), 2 deletions(-)

diff --git a/batch.comments.inc b/batch.comments.inc
index 147de41..9fbf83f 100644
--- a/batch.comments.inc
+++ b/batch.comments.inc
@@ -24,7 +24,7 @@ function blogger_importer_trigger_comments_batch($form, &$form_state) {
 
   // make sure the node type allows for anonymous comments
   if (!user_access('post comments', drupal_anonymous_user())) {
-    return drupal_set_message(t("Anonymoys user does not have permissions to post comments. Please !link to import comments.",
+    return drupal_set_message(t("Anonymous user does not have permissions to post comments. Please !link to import comments.",
       array('!link' => l('correct this', 'admin/people/permissions') )
       ), 'error');
   }
diff --git a/batch.nodes.inc b/batch.nodes.inc
index 4723685..e5389f2 100644
--- a/batch.nodes.inc
+++ b/batch.nodes.inc
@@ -39,7 +39,7 @@ function blogger_importer_trigger_nodes_batch($form, &$form_state) {
 
   // make sure the node type allows for anonymous comments
   if (!user_access('post comments', drupal_anonymous_user())) {
-    return drupal_set_message(t("Anonymoys user does not have permissions to post comments. Please !link to import comments.",
+    return drupal_set_message(t("Anonymous user does not have permissions to post comments. Please !link to import comments.",
       array('!link' => l('correct this', 'admin/people/permissions') )
       ), 'error');
   }
